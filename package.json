{
  "name": "steem.online",
  "version": "1.0.0",
  "description": "Everything you need about steem",
  "main": "server.js",
  "scripts": {
    "test": "npm run lint",
    "start": "node ./server.js",
    "lintfix": "eslint \"**/*.js\" --fix",
    "lint": "eslint \"**/*.js\"",
    "lintstaged": "lint-staged",
    "build": "webpack",
    "dbuild": "set NODE_ENV=debug && webpack",
    "qbuild": "set NODE_ENV=development && webpack",
    "pbuild": "set NODE_ENV=production && webpack",
    "serve": "node ./server.js",
    "nserve": "nodemon ./server.js",
    "dserve": "set NODE_ENV=debug && webpack && npm run serve",
    "qserve": "set NODE_ENV=development && webpack && npm run serve",
    "pserve": "set NODE_ENV=production && webpack && npm run serve",
    "stage": "webpack && npm test && node ./server.js",
    "prettier": "prettier r --config .prettierrc --write \"**/*.js*\"",
    "pretty-quick": "pretty-quick --staged",
    "kill": "taskkill /f /im node.exe"
  },
  "keywords": [
    "steem",
    "steem.online"
  ],
  "author": "dzivenu",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/runtime": "^7.1.5",
    "babel-loader": "^8.0.4",
    "body-parser": "^1.18.2",
    "clean-webpack-plugin": "^1.0.0",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^1.0.1",
    "dotenv": "^6.1.0",
    "dsteem": "^0.11.2",
    "eslint": "^5.9.0",
    "eslint-plugin-jest": "^22.1.0",
    "eslint-plugin-react": "^7.11.1",
    "expose-loader": "^0.7.5",
    "express": "^4.16.2",
    "file-loader": "^2.0.0",
    "husky": "^1.2.0",
    "jquery": "^3.3.1",
    "lint-staged": "^8.1.0",
    "mbuilder": "0.0.2",
    "mini-css-extract-plugin": "^0.4.5",
    "mtools": "0.0.2",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "sitemap": "^1.13.0",
    "style-loader": "^0.23.1",
    "timeago.js": "^4.0.0-beta.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "uikit": "^3.0.0-rc.25",
    "url-loader": "^1.1.2",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2"
  }
}
